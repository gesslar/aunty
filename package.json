{
  "name": "@gesslar/aunty",
  "version": "0.17.0",
  "displayName": "Aunty Rose",
  "description": "Make gorgeous themes that speak as boldly as you do.",
  "publisher": "gesslar",
  "license": "Unlicense",
  "type": "module",
  "main": "src/cli.js",
  "bin": "src/cli.js",
  "files": [
    "src/",
    "UNLICENSE.txt"
  ],
  "scripts": {
    "clean": "rm -rfv ./dist",
    "build": "npm run clean && mkdir -pv ./dist && npm pack --pack-destination ./dist/",
    "exec": "node ./src/cli.js",
    "lint": "eslint src/",
    "submit": "npm publish --access public",
    "examples": "node ./examples/validator.js",
    "test": "node -p \"require('fs').readFileSync('TESTING.txt', 'utf8')\""
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gesslar/aunty"
  },
  "keywords": [
    "asmr",
    "generator",
    "huffing",
    "json5",
    "sniffsniffsniff",
    "taylorswift",
    "theme",
    "vs code",
    "vscode",
    "yaml"
  ],
  "dependencies": {
    "@gesslar/colours": "^0.0.1",
    "chokidar": "^4.0.3",
    "color-support": "^1.1.3",
    "commander": "^14.0.0",
    "culori": "^4.0.2",
    "globby": "^14.1.0",
    "json5": "^2.2.3",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.2.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.28.0",
    "eslint-plugin-jsdoc": "^50.6.17"
  }
}
